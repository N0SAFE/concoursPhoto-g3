{
  "name": "concoursPhoto-g3",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "deployment": "npm run install:prod && concurrently \"npm run migration\" \"npm run build\"",
    "migration": "npm run migration:api",
    "migration:api": "cd api && php bin/console doctrine:migration:migrate",
    "build": "npm run build:client",
    "build:client": "cd client && npm run build",
    "install": "npm run install:api && npm run install:client",
    "install:api": "cd api && composer install",
    "install:client": "cd client && npm install",
    "install:prod": "npm run install:api:prod && npm run install:client",
    "install:api:prod": "cd api && composer install --no-dev --optimize-autoloader",
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:client\"",
    "dev:api": "cd api && symfony server:start --port=8000",
    "dev:client": "cd client && npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/N0SAFE/concoursPhoto-g3.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/N0SAFE/concoursPhoto-g3/issues"
  },
  "homepage": "https://github.com/N0SAFE/concoursPhoto-g3#readme",
  "dependencies": {
    "concurrently": "^7.6.0"
  }
}
