{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "src/*"
            ],
            "@@/*": [
                "/*"
            ],
            "~/*": [
                "src/*"
            ],
            "~~/*": [
                "/*"
            ],
            "@components/*": [
                "src/components/*"
            ],
            "@views/*": [
                "src/views/*"
            ],
            "@assets/*": [
                "src/assets/*"
            ]
        }
    }
}